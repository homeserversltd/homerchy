{
  "packages": [
    "zsh",
    "micro",
    "fastfetch",
    "zoxide",
    "atuin",
    "oh-my-posh",
    "nnn",
    "fzf",
    "eza",
    "bat",
    "bc",
    "tree",
    "net-tools",
    "usbutils",
    "pciutils",
    "ripgrep",
    "git",
    "yay",
    "chezmoi",
    "inetutils",
    "moar"
  ],
  "deployments": [
    {
      "source": "dot/.aliases",
      "destination": "~/.aliases",
      "type": "dotfile",
      "permissions": "644"
    },
    {
      "source": "dot/.functions",
      "destination": "~/.functions",
      "type": "dotfile",
      "permissions": "644"
    },
    {
      "source": "dot/.inputrc",
      "destination": "~/.inputrc",
      "type": "dotfile",
      "permissions": "644"
    },
    {
      "source": "dot/.nanorc",
      "destination": "~/.nanorc",
      "type": "dotfile",
      "permissions": "644"
    },
    {
      "source": "dot/.profile",
      "destination": "~/.profile",
      "type": "dotfile",
      "permissions": "644"
    },
    {
      "source": "dot/.zshrc",
      "destination": "~/.zshrc",
      "type": "dotfile",
      "permissions": "644"
    },
    {
      "source: onmachine/onmachine/onmachine/onmachine/bin/cycle-apps",
      "destination: ~/onmachine/onmachine/onmachine/onmachine/bin/cycle-apps",
      "type": "script",
      "permissions": "755"
    },
    {

      "type": onmachine/onmachine/config",
      "permissions": "644"
    },
    {
      "source": "hypr/hyprlock.conf",
      "destination: ~/.onmachine/onmachine/onmachine/onmachine/config/hypr/hyprlock.conf",
      "type": onmachine/onmachine/config",
      "permissions": "644"
    },
    {

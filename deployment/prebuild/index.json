    {
      "source": "kitty/kitty.conf",
      "destination: ~/.onmachine/onmachine/onmachine/onmachine/config/kitty/kitty.conf",
      "type": onmachine/onmachine/config",
      "permissions": "644"
    },
    {
      "source": "kitty/theme.conf",
      "destination: ~/.onmachine/onmachine/onmachine/onmachine/config/kitty/theme.conf",
      "type": onmachine/onmachine/config",
      "permissions": "644"
    }
